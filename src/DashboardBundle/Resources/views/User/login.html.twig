{% extends "DashboardBundle:Layouts:layout.html.twig"%}
{% block stylessheets %}
	{{ parent() }}

{% endblock %}
{% block content %}
	{{ parent() }}
<div class="col-lg-5 box-form"> 
	 <p class="h4 text-center mb-4">Login</p>
	<hr/>
	
	
		
	<form  class ="md-form" action="{{ path('login_check') }}" method="post">
		<div class="md-form">
<label for="username">Username:</label>
<input type="text" id="username" name="_username" />
</div>
<div class="md-form">
<label for="password">Password:</label>
<input type="password" id="password" name="_password" />
</div>
<div class="md-form">
	<center>
<button  class= "btn-outline-blue-grey "type="submit">LOGIN</button>
<input  type="hidden" name="_target_path" value="/login"/>
</div>
</center>
</form>

</div>
	
{% endblock%}
